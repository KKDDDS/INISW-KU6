   "source": [
    "def ngram(text, n=2):\n",
    "    result = []\n",
    "    for i in range(len(text)-(n-1)):\n",
    "        result.append(text[i:i+n])\n",
    "    return result\n",
    "ngram('아버지가방에들어가신다',1), ngram('아버지가방에들어가신다',2)"
   ]
